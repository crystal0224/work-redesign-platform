  // íŒŒì¼ ë‚´ìš© ì½ê¸° í•¨ìˆ˜ (ê°„ë‹¨í•œ DOCX íŒŒì‹± í¬í•¨)
  const readFileContent = async (file: File): Promise<string> => {
    return new Promise((resolve, reject) => {
      const reader = new FileReader();
      const ext = file.name.split('.').pop()?.toLowerCase();

      reader.onload = async (e) => {
        try {
          const result = e.target?.result;

          if (ext === 'docx' && result instanceof ArrayBuffer) {
            try {
              // DOCX íŒŒì¼ì„ ZIPìœ¼ë¡œ ì½ì–´ì„œ document.xmlì—ì„œ í…ìŠ¤íŠ¸ ì¶”ì¶œ
              const arrayBuffer = result;
              const uint8Array = new Uint8Array(arrayBuffer);

              // ZIP íŒŒì¼ ì‹œê·¸ë‹ˆì²˜ í™•ì¸ (DOCXëŠ” ZIP ê¸°ë°˜)
              if (uint8Array[0] === 0x50 && uint8Array[1] === 0x4B) {
                // ZIP íŒŒì¼ì´ ë§ìœ¼ë©´ ê°„ë‹¨í•œ í…ìŠ¤íŠ¸ ì¶”ì¶œ ì‹œë„
                const textContent = await extractTextFromDocx(arrayBuffer);
                if (textContent && textContent.trim().length > 0) {
                  resolve(textContent);
                  return;
                }
              }

              // í…ìŠ¤íŠ¸ ì¶”ì¶œ ì‹¤íŒ¨ ì‹œ ìƒ˜í”Œ ì—…ë¬´ ìƒì„±ìš© í”Œë˜ê·¸
              resolve('DOCX_PLACEHOLDER_FOR_SAMPLE_TASKS');
            } catch (docxError) {
              console.warn('DOCX íŒŒì‹± ì‹¤íŒ¨, ìƒ˜í”Œ ì—…ë¬´ ìƒì„±:', docxError);
              resolve('DOCX_PLACEHOLDER_FOR_SAMPLE_TASKS');
            }
          } else if (ext === 'txt' && typeof result === 'string') {
            resolve(result);
          } else if (['pdf', 'xlsx', 'xls'].includes(ext || '')) {
            // ë‹¤ë¥¸ ë°”ì´ë„ˆë¦¬ íŒŒì¼ë“¤ì€ ìƒ˜í”Œ ì—…ë¬´ ìƒì„±
            resolve(`${ext?.toUpperCase()}_PLACEHOLDER_FOR_SAMPLE_TASKS`);
          } else if (typeof result === 'string') {
            resolve(result);
          } else {
            resolve('BINARY_PLACEHOLDER_FOR_SAMPLE_TASKS');
          }
        } catch (error) {
          reject(error);
        }
      };

      reader.onerror = () => reject(new Error('íŒŒì¼ ì½ê¸° ì‹¤íŒ¨'));

      if (['docx', 'xlsx', 'pdf'].includes(ext || '')) {
        reader.readAsArrayBuffer(file);
      } else {
        reader.readAsText(file, 'UTF-8');
      }
    });
  };

  // í–¥ìƒëœ DOCX í…ìŠ¤íŠ¸ ì¶”ì¶œ í•¨ìˆ˜
  const extractTextFromDocx = async (arrayBuffer: ArrayBuffer): Promise<string> => {
    try {
      const uint8Array = new Uint8Array(arrayBuffer);

      // ZIP íŒŒì¼ ì‹œê·¸ë‹ˆì²˜ í™•ì¸ (DOCXëŠ” ZIP ê¸°ë°˜)
      if (uint8Array.length < 4 || uint8Array[0] !== 0x50 || uint8Array[1] !== 0x4B) {
        console.warn('ìœ íš¨í•˜ì§€ ì•Šì€ DOCX íŒŒì¼ í˜•ì‹');
        return generateSampleTasks();
      }

      // ë°”ì´ë„ˆë¦¬ ë°ì´í„°ë¥¼ ë¬¸ìì—´ë¡œ ë³€í™˜ (UTF-8ê³¼ Latin-1 ëª¨ë‘ ì‹œë„)
      let textContent = '';
      try {
        textContent = new TextDecoder('utf-8', { fatal: false }).decode(uint8Array);
      } catch {
        textContent = new TextDecoder('latin-1', { fatal: false }).decode(uint8Array);
      }

      // document.xml ì„¹ì…˜ ì°¾ê¸° (DOCXì˜ ì£¼ ë¬¸ì„œ ë‚´ìš©)
      const docXmlMatch = textContent.match(/document\.xml.*?<\/pkg:part>/s);
      if (docXmlMatch) {
        textContent = docXmlMatch[0];
      }

      // XML íƒœê·¸ ë‚´ í…ìŠ¤íŠ¸ ì¶”ì¶œì„ ìœ„í•œ ë‹¤ì–‘í•œ íŒ¨í„´
      const patterns = [
        /<w:t[^>]*>([^<]+)<\/w:t>/g,  // Word í…ìŠ¤íŠ¸ íƒœê·¸
        /<text[^>]*>([^<]+)<\/text>/g,  // ì¼ë°˜ í…ìŠ¤íŠ¸ íƒœê·¸
        />[\s]*([ê°€-í£a-zA-Z0-9][^<]*[ê°€-í£a-zA-Z0-9])[\s]*</g,  // í•œê¸€/ì˜ë¬¸ì´ í¬í•¨ëœ í…ìŠ¤íŠ¸
        />([^<]{10,})</g  // 10ì ì´ìƒì˜ ê¸´ í…ìŠ¤íŠ¸
      ];

      const extractedLines = new Set<string>();

      for (const pattern of patterns) {
        let match;
        while ((match = pattern.exec(textContent)) !== null) {
          const text = match[1]?.trim();
          if (text && isValidTaskText(text)) {
            extractedLines.add(text);
          }
        }
      }

      // ì¶”ì¶œëœ ë¼ì¸ë“¤ì„ ì •ë¦¬
      const validLines = Array.from(extractedLines)
        .filter(line => line.length >= 5 && line.length <= 200)
        .slice(0, 15);

      if (validLines.length >= 3) {
        console.log('DOCXì—ì„œ ì¶”ì¶œëœ ì—…ë¬´ë“¤:', validLines);
        return validLines.join('\n');
      }

      return generateSampleTasks();
    } catch (error) {
      console.error('DOCX íŒŒì‹± ì˜¤ë¥˜:', error);
      return generateSampleTasks();
    }
  };

  // ìœ íš¨í•œ ì—…ë¬´ í…ìŠ¤íŠ¸ì¸ì§€ í™•ì¸
  const isValidTaskText = (text: string): boolean => {
    if (!text || text.length < 5 || text.length > 200) return false;

    // í•œê¸€ ë˜ëŠ” ì˜ë¬¸ì´ ìµœì†Œ 3ì ì´ìƒ í¬í•¨ë˜ì–´ì•¼ í•¨
    const hasValidContent = /[ê°€-í£]{3,}|[a-zA-Z]{3,}/.test(text);
    if (!hasValidContent) return false;

    // ì œì™¸í•  íŒ¨í„´ë“¤
    const excludePatterns = [
      /^[\d\s\-_.,:;!@#$%^&*()+=\[\]{}|\\`~"'<>?/\u00a0\u2000-\u200f\u2028-\u202f]*$/,  // ìˆ«ìì™€ íŠ¹ìˆ˜ë¬¸ìë§Œ
      /^(xmlns|encoding|version|standalone|docProps|word|document|pkg|Content_Types|rels|theme|styles|settings|fontTable|numbering)/i,  // XML ê´€ë ¨
      /^[\s\n\r\t\u00a0\u2000-\u200f\u2028-\u202f]+$/,  // ê³µë°±ë¬¸ìë§Œ
      /^\d{1,4}$/,  // ë‹¨ìˆœ ìˆ«ì
